<% content_for :page_header do -%>
  <%= render "filter" %>
<% end %>
<% if collection.any? %>
  <div class="row">
    <%= render collection %>
  </div>
  <div class="row">
    <div class="col-sm-12 js-pagination">
      <%= will_paginate %>
    </div> 
  </div> 
<% else %>
  <div class="row">
    <div class="col-xs-12">
      <%= render "no_records_found" %>
    </div>
  </div>
<% end %>